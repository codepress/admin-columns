{
    "name": "admincolumns",
    "version": "1.0.0",
    "description": "Sass and JS for Admin Columns",
    "private": true,
    "devDependencies": {
        "@babel/core": "7.10.2",
        "@babel/preset-typescript": "^7.3.3",
        "@types/jquery": "^3.5.5",
        "@types/node": "^14.14.37",
        "axios": "^0.21.0",
        "babel-loader": "^8.0.5",
        "body-scroll-lock": "^4.0.0-beta.0",
        "jquery": "^3.5.1",
        "js-cookie": "^2.2.1",
        "nanobus": "4.4.0",
        "nps": "^5.9.12",
        "nps-utils": "^1.7.0",
        "sass": "^1.32.12",
        "svelte": "^3.42.6",
        "svelte-check": "^2.2.4",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.6.5",
        "svg-symbols": "1.0.4",
        "svgstore-cli": "1.3.1",
        "tailwindcss": "^3.2.4",
        "ts-loader": "^8.0.14",
        "typescript": "^4.2",
        "webfont": "8.1.4",
        "webpack": "^5.19.0",
        "webpack-cli": "^4.5.0",
        "wp-pot": "latest"
    },
    "scripts": {
        "build": "nps build",
        "ac:build:development": "nps build.development",
        "ac:tailwind": "npx tailwindcss -i ./tailwind/ac-utilities.css -o ../assets/css/utilities.css --watch",
        "ac:languages": "nps languages",
        "ac:webfont": "node ./scripts/webfont.js",
        "ac:svgsymbols": "svgstore ./svg-symbols/*.svg -o ../assets/images/symbols.svg"
    },
    "webfont": {
        "formats": [
            "woff2",
            "woff"
        ],
        "template": "./webfont/template.scss.njk",
        "fontName": "cpac_icons",
        "fontHeight": "1000",
        "templateFontPath": "../fonts/",
        "templateClassName": "cpacicon",
        "normalize": true
    },
    "dependencies": {
        "svelte-json-tree": "^1.0.0"
    }
}
