{
    "name": "admincolumns",
    "version": "1.0.0",
    "description": "Sass and JS for Admin Columns",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "@babel/core": "7.22.20",
        "@babel/preset-typescript": "7.22.15",
        "@tailwindcss/container-queries": "^0.1.1",
        "@types/jquery": "^3.5.5",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.6.2",
        "axios": "^1.6.5",
        "babel-loader": "9.1.3",
        "body-scroll-lock": "^4.0.0-beta.0",
        "jquery": "^3.5.1",
        "js-cookie": "^2.2.1",
        "nanobus": "4.5.0",
        "nps": "^5.9.12",
        "nps-utils": "^1.0.2",
        "sass": "^1.32.12",
        "svelte": "4.2.0",
        "svelte-check": "3.5.1",
        "svelte-loader": "3.1.9",
        "svelte-preprocess": "5.0.4",
        "svg-symbols": "1.0.4",
        "svgstore-cli": "^2.0.1",
        "tailwindcss": "^3.4.17",
        "ts-loader": "^9.4.4",
        "typescript": "^5.2.2",
        "webfont": "^11.0.2",
        "webpack": "^5.19.0",
        "webpack-cli": "^4.5.0",
        "wp-pot": "latest"
    },
    "scripts": {
        "build": "nps build",
        "ac:build:development": "nps build.development",
        "ac:tailwind": "npx tailwindcss -i ./tailwind/ac-utilities.css -o ../assets/css/utilities.css --watch",
        "ac:languages": "nps languages",
        "ac:webfont": "node ./scripts/webfont.js",
        "ac:svgsymbols": "svgstore ./svg-symbols/*.svg -o ../assets/images/symbols.svg"
    },
    "webfont": {
        "formats": [
            "woff2",
            "woff"
        ],
        "template": "./webfont/template.scss.njk",
        "fontName": "cpac_icons",
        "fontHeight": "1000",
        "templateFontPath": "../fonts/",
        "templateClassName": "cpacicon",
        "normalize": true
    },
    "dependencies": {
        "@ac/material-icons": "file:admin-columns/src/packages/material-icons",
        "@wordpress/i18n": "^5.9.0",
        "deep-clone": "^4.0.0",
        "lodash-es": "^4.17.21",
        "svelte-json-tree": "^1.0.0",
        "svelte-select": "5.7.0"
    }
}
